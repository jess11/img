<div class="mainGalleryWrapper">
  <% @photos.each do |photo| %>
      <div style='background-image: url( "http://res.cloudinary.com/kryx11/image/upload/<%=photo.image%>")', class="photo">
          <%= link_to photo_path(:id => photo.id), :method => 'delete', :data => { :confirm => 'Are you sure?'} do%><i class="glyphicon glyphicon-remove delete" aria-hidden="true"></i><% end %>
          <div class="photoGalleryLabel"><p><%=photo.name%></p></div>

      </div>
  <% end %>
</div>
